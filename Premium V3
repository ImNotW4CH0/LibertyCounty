    Suffix = "FireRate",
    CurrentValue = 1,
    Flag = "FireRateSlider",
    Callback = function(Value)
        for i, v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
            if v:FindFirstChild("WeaponRoot") then
                local a1 = require(game:GetService("ReplicatedStorage").Shared.WeaponStats[v.Name])
                a1["FireRate"] = Value
            else
                 for i, v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
                    if v:FindFirstChild("WeaponRoot") then
                        local a2 = require(game:GetService("ReplicatedStorage").Shared.WeaponStats[v.Name])
                        a2["FireRate"] = Value
                    end
                 end
            end
        end
     end,
})

