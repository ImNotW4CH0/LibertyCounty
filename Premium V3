local Players = game:GetService("Players")
local RunService = game:GetService("RunService")

local localPlayer = Players.LocalPlayer
local walkflinging = false
local noclipping = nil
local noclipConnection = nil

local function getRoot(character)
    return character:FindFirstChild("HumanoidRootPart") or character:FindFirstChildOfClass("BasePart")
end
local function toggleNoclip()
    noclipping = not noclipping

    if noclipping then
        noclipConnection = RunService.Stepped:Connect(function()
            if localPlayer.Character then
                for _, part in pairs(localPlayer.Character:GetDescendants()) do
                    if part:IsA("BasePart") and part.CanCollide then
                        part.CanCollide = false
                    end
                end
            end
        end)
    else
        if noclipConnection then noclipConnection:Disconnect() end
    end
end

local function enableNoclip()
    noclipping = RunService.Stepped:Connect(function()
        if localPlayer.Character then
            for _, part in pairs(localPlayer.Character:GetDescendants()) do
                if part:IsA("BasePart") and part.CanCollide then
                    part.CanCollide = false
                end
            end
        end
    end)
end

local function disableNoclip()
    if noclipping then noclipping:Disconnect() end
end

local function toggleWalkFling()
    walkflinging = not walkflinging

    if not walkflinging then
        disableNoclip()
        return
    end

    enableNoclip()

    task.spawn(function()
        while walkflinging do
            RunService.Heartbeat:Wait()

            local character = localPlayer.Character
            local root = getRoot(character)
            local velocity, movel = nil, 0.1

            while not (character and root) do
                RunService.Heartbeat:Wait()
                character = localPlayer.Character
                root = getRoot(character)
            end

            velocity = root.Velocity
            root.Velocity = velocity * 1e6 + Vector3.new(0, 1e6, 0)

            RunService.RenderStepped:Wait()
            if character and root then
                root.Velocity = velocity
            end

            RunService.Stepped:Wait()
            if character and root then
                root.Velocity = velocity + Vector3.new(0, movel, 0)
                movel = -movel
            end
        end
    end)
end

local Players = game:GetService("Players")
local Mouse = Players.LocalPlayer:GetMouse()
local FLYING = false
local QEfly = true
local flySpeed = 1 -- Default speed
local maxFlySpeed = 30 -- Maximum speed for slider

local function getRoot(char)
    return char:FindFirstChild("HumanoidRootPart") or char:FindFirstChildOfClass("BasePart")
end

function sFLY(vfly)
    repeat task.wait() until Players.LocalPlayer and Players.LocalPlayer.Character and getRoot(Players.LocalPlayer.Character) and Players.LocalPlayer.Character:FindFirstChildOfClass("Humanoid")

    local T = getRoot(Players.LocalPlayer.Character)
    local CONTROL = {F = 0, B = 0, L = 0, R = 0, Q = 0, E = 0}
    local SPEED = 0

    local function FLY()
        FLYING = true
        local BG = Instance.new("BodyGyro", T)
        local BV = Instance.new("BodyVelocity", T)
        BG.P = 9e4
        BG.maxTorque = Vector3.new(9e9, 9e9, 9e9)
        BV.maxForce = Vector3.new(9e9, 9e9, 9e9)

        task.spawn(function()
            repeat task.wait()
                local direction = (workspace.CurrentCamera.CFrame.LookVector * (CONTROL.F + CONTROL.B)) +
                                  (workspace.CurrentCamera.CFrame.RightVector * (CONTROL.L + CONTROL.R)) +
                                  Vector3.new(0, CONTROL.Q + CONTROL.E, 0)
                BV.velocity = direction * SPEED
                BG.CFrame = workspace.CurrentCamera.CFrame
                SPEED = (CONTROL.F + CONTROL.B + CONTROL.L + CONTROL.R + CONTROL.Q + CONTROL.E ~= 0) and flySpeed or 0
            until not FLYING

            BG:Destroy()
            BV:Destroy()
        end)
    end

    Mouse.KeyDown:Connect(function(key)
        key = key:lower()
        if key == "w" then CONTROL.F = flySpeed elseif key == "s" then CONTROL.B = -flySpeed end
        if key == "a" then CONTROL.L = -flySpeed elseif key == "d" then CONTROL.R = flySpeed end
        if QEfly and key == "e" then CONTROL.Q = flySpeed * 2 elseif QEfly and key == "q" then CONTROL.E = -flySpeed * 2 end
    end)

    Mouse.KeyUp:Connect(function(key)
        key = key:lower()
        if key == "w" or key == "s" then CONTROL.F, CONTROL.B = 0, 0 end
        if key == "a" or key == "d" then CONTROL.L, CONTROL.R = 0, 0 end
        if key == "e" or key == "q" then CONTROL.Q, CONTROL.E = 0, 0 end
    end)

    FLY()
end

function NOFLY()
    FLYING = false
end

local function spoofVehicleIdentity(Thing, Index, Value)
    for i, v in pairs(workspace.Vehicles:GetChildren()) do
        if v["Control_Values"].Owner.Value == game.Players.LocalPlayer.Name then
            local module = require(v["Drive Controller"])
            
            if module[Thing] then
                module[Thing][Index] = Value
            else
                --print("Error: '"..Thing.."' not found in the module.")
            end
        end
    end
end

local function spoofVehicleIdentity2(Thing, Value)
    for i, v in pairs(workspace.Vehicles:GetChildren()) do
    if v["Control_Values"].Owner.Value == game.Players.LocalPlayer.Name then
        local module = require(v["Drive Controller"])
        module[Thing] = Value
        print("Succesfully spoofed ".. v.Name.. " ".. Thing.. " to ".. Value .. "!" )
        print("Current value of ".. Thing.." is "..module[Thing])
    end
end
end

