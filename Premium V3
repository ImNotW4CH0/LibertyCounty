local walkSpeed = 16
local sprintSpeed = 50
local jumpPower = 50
local isSprinting = false

local Section1 = Tab1:CreateSection("Humanoid")
local Section2 = Tab2:CreateSection("Mods")
local Section3 = Tab3:CreateSection("Robberies")
