local Button11 = Tab5:CreateButton({
    Name = "Teleport to nearest Lockpick House Bill",
    Callback = function()
        local houseRobbery = workspace:FindFirstChild("HouseRobbery")
        
        if houseRobbery and #houseRobbery:GetChildren() > 0 then
            local atms = houseRobbery:GetChildren()
            local playerPosition = game.Players.LocalPlayer.Character.HumanoidRootPart.Position
            local nearestATM = nil
            local shortestDistance = math.huge
            local hasBills = false  -- Track if any corresponding bills are found
            
            for _, atm in pairs(atms) do
                -- Ensure 'StartingPlayer' exists and matches the LocalPlayer
                if atm:FindFirstChild("StartingPlayer") and atm.StartingPlayer.Value == game.Players.LocalPlayer then
                    hasBills = true  -- Set this to true if the player has bills
                    local targetPart = atm:FindFirstChild("Part")  -- Replace 'Part' with the correct part name
                    if targetPart then
                        local distance = (targetPart.Position - playerPosition).Magnitude
                        if distance < shortestDistance then
                            shortestDistance = distance
                            nearestATM = targetPart
                        end
                    end
                end
            end

            if nearestATM then
                task.wait(0.5)
                -- Teleport to the nearest ATM part
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = nearestATM.CFrame
            elseif not hasBills then
                -- Notify only if no bills were found
                Rayfield:Notify({
                    Title = "Cheat Notification",
                    Content = "There are no robbery bills corresponding to you.",
                    Duration = 6.5,
                    Image = 4483362458,
                    Actions = { Ignore = { Name = "Aw!", Callback = function() end } },
                })
            end
        else
            -- No bills in the robbery area at all
            Rayfield:Notify({
                Title = "Cheat Notification",
                Content = "No robbery bills found.",
                Duration = 6.5,
                Image = 4483362458,
                Actions = { Ignore = { Name = "Aw!", Callback = function() end } },
            })
        end
    end,
})
