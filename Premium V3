                    while v and v.Character and v.Character:FindFirstChild("HumanoidRootPart") do
                        local target = v.Character.HumanoidRootPart
                        local myChar = Players.LocalPlayer.Character
                        if target and myChar then
                            myChar.HumanoidRootPart.CFrame = target.CFrame
                        end
                        FE:WaitForChild("UseHandcuffs"):InvokeServer("Handcuff", v)
                        task.wait()
                        FE:WaitForChild("UseHandcuffs"):InvokeServer("Arrest", v)
                        if FE:WaitForChild("GetWantedLevel"):InvokeServer(v) == 0 then
                            break
                        end
                        task.wait()
                    end
                end
            end
            task.wait()
        end
    end,
})

local Button8 = Tab5:CreateButton({
    Name = "Teleport to nearest house",
    Callback = function()
        local houses = workspace.Houses:GetChildren()
        local playerPosition = game.Players.LocalPlayer.Character.HumanoidRootPart.Position
        local nearestHouse = nil
        local shortestDistance = math.huge
        
        for _, house in pairs(houses) do
            local distance = (house.WorldPivot.Position - playerPosition).Magnitude
            if distance < shortestDistance then
                shortestDistance = distance
                nearestHouse = house
            end
        end

        if nearestHouse then
            task.wait(0.5)
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = nearestHouse.WorldPivot
        end
    end,
})

